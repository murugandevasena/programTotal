package pattern;

public class PatternPractice {
	public static void main(String[] args) {
		int x = 5;
		for (int i = 1; i <= x; i++) { 
			for(int k=1;k<=i;k++) {
				System.out.print(" ");
			}
			for (int j = i; j <= x; j++) {
				System.out.print("*"+" ");
			}
			//System.out.println(" ");
		}
	}
}
